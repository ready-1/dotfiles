export PATH="$HOME/.local/bin/scripts:$PATH"
export PATH="$HOME/.local/bin/scripts/utils:$PATH"
export PATH="$HOME/.local/bin/scripts/network:$PATH"
export PATH="$HOME/.local/bin/scripts/dev:$PATH"
export LANG="en_US.UTF-8"


export GPG_TTY=$(tty)




